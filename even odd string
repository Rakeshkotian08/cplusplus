#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
#include <bits/stdc++.h> 
using namespace std;


int main() {
   
    int n;
    string s;
    cin>>n;
    for(int i=0;i<n;i++)
    {
        cin>>s;
        char ch[s.length()];
        strcpy(ch,s.c_str());
        for(int j=0;j<s.length();j=j+2)
        {
            cout<<ch[j];
        }
        cout<<" ";
        for(int k=1;k<s.length();k=k+2)
        {
            cout<<ch[k];
        }
        cout<<"\n";

    }  
  
    return 0;
}
